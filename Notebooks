import java.util.*;
public class Notebook {

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int N=sc.nextInt();
		int Arr[]=new int[N];
		for(int i=0;i<N;i++)
		{
			Arr[i]=sc.nextInt();
		}
		for(int i=0;i<N;i++)
		{
			Arr[i]=Arr[i]*1000/100;
			System.out.println(Arr[i]);
			sc.close();
		}
}
}
