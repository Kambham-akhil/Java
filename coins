import java.util.Scanner;

public class Coins {

	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		int Arr[]=new int[t];
		for(int i=0;i<t;i++)
		{
			while(t!=0)
			{
				int X=sc.nextInt();
			    int Y=sc.nextInt();
			    System.out.println((X*10)+(Y*5));
			    t--;
          sc.close();
			}
		}
	}
}
