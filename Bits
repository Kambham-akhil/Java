import java.util.Scanner;

public class N_Bits {

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int X=sc.nextInt();
		int Arr[]=new int[X];
		for(int i=0;i<X;i++)
		{
			Arr[i]=sc.nextInt();
		}
		for(int i=0;i<X;i++)
		{
			if(Arr[i]%4==0)
				{
					System.out.println("Good");
				}
				else
				{
					System.out.println("Not Good");
					sc.close();
				}
		}
		}
		}
